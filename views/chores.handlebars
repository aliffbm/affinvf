   	<div class="container" id="choreContainer">
   		<div class="row">

   			<div class="col-md-6">
   				<div>
   					<form class="form" id="new-chore-form">
   						<div class="form-group">
   							<label class="control-label" for="choreTitle">Chore Name: </label>
   							<input type="text" class="form-control" id="choreTitle" placeholder="i.e. Dishes, Bathroom, Etc." />
   						</div>
   						<div class="form-group">
   							<label class="control-label" for="expectedTime">Timeframe: </label>
   							<input type="text" class="form-control" id="expectedTime" placeholder="i.e. 4 days" />
   						</div>

   						<div class="form-group">
   							<label class="control-label" for="imageURL">Image URL: </label>
   							<input type="text" class="form-control" id="imageURL" placeholder="http://www.example.com/pic_of_chore.jpg" />
   						</div>
   						<div class="form-group">
   							<button id="newChoreSubmitButton" class="btn btn-default" type="button">Add Chore</button>
   						</div>
   					</form>
   				</div>

   			</div>
   			<div class="col-md-6">
   	<!-- 		{{#each emoticomment}}
   			<img src="{{image}}"/>
   			{{/each}} -->

   				{{#each chores}}

   				<div class="panel panel-default" id={{id}}>
   					<div class="panel-heading">
   						
   						<div class="pull-left" style="font-size: 20px; ">
   						{{name}}
   						<!-- Trigger/Open The Modal -->
   						</div>
   					
   					
   						<div><img src=""></div>

   						<span class="pull-right text-uppercase glyphicon glyphicon-remove chore-delete"></span>
   					</div>

   					<div class="panel-body">
   						{{expectedTime}}
   					</div>
   					<div class="panel-body">
   						<img class="img-responsive" src="{{image}}"/>

   					</div>
   				</div>
   				{{/each}}


   			</div>

   		</div>
   	</div>

